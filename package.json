  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "format": "wp-scripts format ./src",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js ./src",
    "plugin-zip": "wp-scripts plugin-zip",
    "env": "wp-env",
    "env:start": "wp-env start --xdebug=coverage",
    "env:start-ci": "wp-env start --debug",
    "env:stop": "wp-env stop",
    "env:reset": "wp-env clean",
    "env:destroy": "wp-env destroy",
    "env:cli": "wp-env run cli",
    "env:tests-cli": "wp-env run tests-cli",
    "pretest:php": "npm run env run tests-wordpress composer update -- -W --working-dir=/var/www/html/wp-content/plugins/wc-bretagne-2024/",
    "test:php": "wp-env run tests-wordpress /var/www/html/wp-content/plugins/wc-bretagne-2024/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/wc-bretagne-2024/phpunit.xml.dist -- --coverage-html=/var/www/html/wp-content/plugins/wc-bretagne-2024/coverage/",
    "test:e2e": "wp-scripts test-playwright --config tests/playwright/playwright.config.js",
    "i18n:make-pot": "npm run env run cli wp i18n make-pot /var/www/html/wp-content/plugins/wc-bretagne-2024/ /var/www/html/wp-content/plugins/wc-bretagne-2024/languages/wc-bretagne-2024.pot -- --exclude=/var/www/html/wp-content/plugins/wc-bretagne-2024/vendor --  --exclude=/var/www/html/wp-content/plugins/wc-bretagne-2024/src",
    "i18n:make-json": "npm run env run cli wp i18n make-json -- --no-purge",
    "packages-update": "wp-scripts packages-update"
  },
  "files": [
    "admin/**",
    "build/**",
    "includes/**",
    "languages/**",
    "public/**",
    "plugin.php",
    "uninstall.php",
    "wc-bretagne-2024.php",
    "block.json",
    "changelog.*",
    "license.*",
    "readme.",
    "vendor"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
